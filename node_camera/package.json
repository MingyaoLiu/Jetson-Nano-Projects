{
  "name": "node_pi_cam",
  "version": "1.0.0",
  "description": "Node web pi cam",
  "main": "dist/server.js",
  "scripts": {
    "watch": "npx nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "npx ts-node src/server.ts",
    "build": "npx webpack --config webpack.config.js --mode production",
    "prebuild": "npx tslint -c tslint.json -p tsconfig.json --fix",
    "test": "npx mocha -r ts-node/register **/*.spec.ts",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MingyaoLiu.git"
  },
  "keywords": [
    "webserver",
    "nodejs",
    "expressjs"
  ],
  "author": "Mingyao Liu",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MingyaoLiu"
  },
  "homepage": "https://github.com/MingyaoLiu",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "mongodb": "^3.5.5",
    "pi-camera-connect": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "^3.5.4",
    "@types/node": "^13.9.8",
    "@types/request": "^2.48.4",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "mocha": "^10.2.0",
    "nodemon": "*",
    "request": "^2.88.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
